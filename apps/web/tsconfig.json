{
  "compilerOptions": {
    "strict": false,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@server/*": [
        "@neo/trpc*"
      ]
    },
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "declarations.d.ts",
    "./src/**/*.ts",
    "./src/**/*.tsx",
    ".next/types",
    ".next/types/**/*.ts",
    "middleware.ts",
    "src/app/(app)/webhooks/webhook-list-item-actions.tsx.bak"
  ],
  "exclude": [
    "node_modules"
  ]
}
